<%- include('partials/head', {tituloWeb: 'Antiria - Home' }) %>

    <body class="body_index">

        <!-- HEADER -->
        <%- include("./partials/header")%>
            <!-- FIN HEADER -->

            <main class="main-index">
                <div class="contenedor">
                    <div class="contenido">
                        <div class="primerSlide">
                            <div class="imagen">
                                <img src="\img\p1.jpg" alt="producto">
                            </div>
                            <div class="descripcion">
                                <h3>
                                    Novedades
                                </h3>
                                <p>
                                    Reliquia:
                                <blockquote>"Contemplando los huesos, o a veces todo el cuerpo, de
                                    alguien que dedicó su vida a Dios de modo tan especial que se convirtió en santo,
                                    nace el
                                    deseo de una vida similar"
                                </blockquote>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="box-ofertas">
                    <h5 class="ofertas">Ofertas</h5>
                </div>
                <section class="contenedor-4">

                    <% oferta.forEach ((row)=> { %>
                        <article class="tarjeta-2">
                            <div class="contenedor-img">
                                <img src="/img/<%= row.image %>" alt="">
                            </div>
                            <div class="detalle-tarjeta">
                                <h3 class="nombre">
                                    <%= row.nombreProd %>
                                </h3>
                                <h4 class="precio">
                                    $ <%= row.precio %>
                                </h4>
                                <p><%= row.descIndex %></p>
                                <div>
                                    <a href="/productDetail/<%= row.id %>">VER MÁS</a>
                                </div>
                            </div>
                        </article>
                        <% }) %>



                </section>
                <h2 class="categoriash2">Marcas emblemáticas</h2>
                <section class="usuarios-vendedores">
                    <a href="">
                        <div class="coin-usuario">
                            <div class="coin-front">
                                <img src="/img/1.png" alt="">
                            </div>
                            <div class="coin-back">
                                <img src="/img/isotipo2.png" alt="">

